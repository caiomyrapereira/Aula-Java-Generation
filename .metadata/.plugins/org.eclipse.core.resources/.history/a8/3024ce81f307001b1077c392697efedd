package Exercicios;

import java.util.Scanner;

public class Exe01 {
	public static void main(String args[]) {
		Scanner ler = new Scanner(System.in);
		
		int idade,anos,meses,dias;
		
		System.out.println("Digite sua idade: ");
		idade = ler.nextInt();
	
		dias = (idade*365)%30; 
		
		System.out.println(dias);
		
	}
}
