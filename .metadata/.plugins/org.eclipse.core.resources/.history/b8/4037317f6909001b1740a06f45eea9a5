package Lista03Exercicios;

import java.util.Scanner;

public class Exe02 {
	public static void main(String args[]) {
		
		int 
		count, 
		quantidadePar = 0, 
		quantidadeImpar = 0,
		numero;
		Scanner ler = new Scanner(System.in);
		
		for(count = 0; count<10; count++) {
			if(count%2==0){
				quantidadePar++;
			}
			else {
				quantidadeImpar++;
			}
		}
		
		System.out.printf("A quantidade de número par é %d", quantidadePar)
	}
}
