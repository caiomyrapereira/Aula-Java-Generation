package PooExe01;

public class Cliente {
	private String nome;
	private String enderenco;
	private int cpf;
	private int idade;
	private float dinheiro;
	
	public Cliente(String nome, int idade, String enderenco, int cpf, double d) {
		this.nome = nome;
		this.idade = idade;
		this.enderenco = enderenco;
		this.cpf = cpf;
		this.dinheiro = d;
	}
	
}
