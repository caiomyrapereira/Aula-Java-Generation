package Lista02Exercicios;

import java.util.Scanner;

public class exe01 {
	public static void main(String args[]) {
		
		Scanner ler = new Scanner(System.in);
		int n1, n2, n3;
			
		System.out.println("Digite um valor qualquer na variavel 'n1' :");
		n1 = ler.nextInt();
		System.out.println("Digite um valor qualquer na variavel 'n2' :");
		n2 = ler.nextInt();
		System.out.println("Digite um valor qualquer na variavel 'n3' :");
		n3 = ler.nextInt();
		
		if( (n1>n2) && (n1>n3) ) {
			System.out.printf("A variavel 'n1' é maior entre 'n2' e 'n2' %d",n1)
		}
		
	}
}
